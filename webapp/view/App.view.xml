<mvc:View controllerName="EntrepinaresUI5.SAPUI5.controller.App" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" >
<Shell>
	<App class="myCustomCss">
		<pages>
			<Page title="{i18n>homePageTitle}">
				<headerContent>
					<Button id="helloDialogHeader" icon="sap-icon://world" press="onOpenDialog" class="sapUiSmallMarginEnd"/>
				</headerContent>
				<content>
					<mvc:XMLView viewName="EntrepinaresUI5.SAPUI5.view.HelloPanel"/>
                    <mvc:XMLView viewName="EntrepinaresUI5.SAPUI5.view.InvoiceList"/>
				</content>
			</Page>
		</pages>
	</App>
</Shell>	
</mvc:View>
